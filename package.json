{
    "name": "vosk-cli",
    "version": "0.1.0",
    "description": "Use vosk in command line.",
    "keywords": [
        "vosk"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "type": "module",
    "bin": {
        "vosk": "dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "pnpm build && node dist/index.js",
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "build": "tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
        "lint": "eslint ."
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "commander": "^9.4.0",
        "mic": "^2.1.2",
        "ora": "^6.1.2",
        "vosk-lib": "^0.1.3",
        "wav": "^1.0.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@types/jest": "^28.1.8",
        "@types/node": "^18.7.15",
        "@types/wav": "^1.0.1",
        "@types/yauzl": "^2.10.0",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
        "ts-jest": "^28.0.8",
        "tsup": "^6.2.3",
        "tsx": "^3.9.0",
        "typedoc": "^0.23.14",
        "typescript": "^4.8.2"
    }
}
